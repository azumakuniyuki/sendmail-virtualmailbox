	rm -f $(PM_ARCHIVE)
	rm -f $(MD_ARCHIVE)
	rm -f $(SM_ARCHIVE)
	rm -f $(SI_ARCHIVE)
	rm -f ./*-ok
	rm -fr ./archive/*
